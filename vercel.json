{
    "builds": [
      {
        "src": "index.js", 
        "use": "@vercel/node"
      },
      {
        "src": "public", 
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "public/"
      },
      {
        "src": "/(.*)",
        "dest": "index.js"
      }
    ]
  }
  